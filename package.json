{
  "name": "demo.armandphilippot.com",
  "description": "A collection of small app demo.",
  "version": "2.0.0",
  "license": "MIT",
  "author": {
    "name": "Armand Philippot",
    "email": "contact@email.com",
    "url": "https://www.armandphilippot.com"
  },
  "private": true,
  "engines": {
    "node": "^16.4.0",
    "yarn": "^1.22.0"
  },
  "scripts": {
    "prebuild": "./config/build.sh",
    "build": "webpack --node-env=production && npm run rtl",
    "fix": "npm-run-all --parallel fix:*",
    "fix:js": "eslint --ext .js,.jsx --fix",
    "fix:php": "composer fix",
    "fix:scss": "stylelint \"**/*.scss\" --fix",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --ext .js,.jsx",
    "lint:php": "composer lint",
    "lint:scss": "stylelint \"**/*.scss\" --syntax scss --formatter verbose",
    "release": "standard-version -a -s",
    "rtl": "rtlcss public/assets/css/style.css public/assets/css/style-rtl.css",
    "preserve": "./config/build.sh",
    "serve": "webpack serve --node-env=development",
    "watch": "webpack --watch --progress --node-env=development"
  },
  "devDependencies": {
    "@babel/core": "^7.17.4",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.2",
    "browserslist": "^4.19.1",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "husky": "^7.0.2",
    "image-minimizer-webpack-plugin": "^3.2.3",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "lint-staged": "^12.3.4",
    "mini-css-extract-plugin": "^2.5.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "postcss-loader": "^6.2.1",
    "prettier": "2.5.1",
    "rtlcss": "^3.5.0",
    "sass": "^1.49.7",
    "sass-loader": "^12.6.0",
    "standard-version": "^9.3.1",
    "style-loader": "^3.3.0",
    "stylelint": "^15.10.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^25.0.0",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "ackee-tracker": "^5.1.0",
    "dotenv": "^16.0.0",
    "dotenv-expand": "^8.0.1",
    "i18n-js": "^3.8.0",
    "modern-normalize": "^1.1.0"
  }
}
